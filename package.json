{
    "name": "node-express-mongodb-jwt-rest-api-skeleton",
    "version": "8.1.1",
    "description": "Node.js express.js MongoDB JWT REST API - This is a basic API REST skeleton written on JavaScript using async/await. Great for building a starter web API for your front-end (Android, iOS, Vue, react, angular, or anything that can consume an API)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/daniellopezj01/back-prestadora.git"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=production pm2 start server.js",
        "mocha": "nyc mocha --timeout=50000 --exit",
        "test": "cross-env NODE_ENV=test npm run fresh && npm run mocha",
        "dev": "cross-env NODE_ENV=development nodemon server.js",
        "fresh": "npm run clean && npm run seed",
        "clean": "node clean.js",
        "seed": "node seed.js",
        "format": "prettier --write --config .prettierrc.json \"**/*.js\"",
        "lint": "eslint --fix --config .eslintrc.json \"**/*.js\"",
        "remark": "remark . -o"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run remark"
        }
    },
    "dependencies": {
        "bcrypt": "5.0.0",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "date-fns": "^2.11.1",
        "dotenv-safe": "^8.2.0",
        "ejs": "^3.0.2",
        "expeditious-engine-redis": "^0.1.2",
        "express": "^4.17.1",
        "express-expeditious": "^5.1.1",
        "express-validator": "^6.4.0",
        "helmet": "^3.22.0",
        "i18n": "^0.8.6",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "lodash.isdate": "^4.0.1",
        "mongoose": "^5.9.6",
        "mongoose-aggregate-paginate-v2": "^1.0.4",
        "mongoose-delete": "^0.5.2",
        "mongoose-paginate-v2": "^1.3.7",
        "morgan": "^1.10.0",
        "node-pre-gyp": "^0.15.0",
        "nodemailer": "^6.4.6",
        "nodemailer-mailgun-transport": "^2.0.0",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "remark-cli": "^8.0.0",
        "request-ip": "^2.1.3",
        "trim-request": "^1.0.6",
        "uuid": "^7.0.3",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "faker": "^4.1.0",
        "mongo-seeding": "3.3.0",
        "validator": "^13.0.0"
    },
    "devDependencies": {
        "cross-env": "^7.0.2",
        "eslint": "^7.14.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-formidable": "^4.0.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.2.5",
        "mocha": "^7.1.1",
        "nodemon": "^2.0.2",
        "nyc": "^15.0.1",
        "prettier": "^2.0.3",
        "prettier-eslint": "^9.0.1"
    },
    "keywords": [
        "javascript",
        "api",
        "node",
        "express",
        "mongo",
        "mongodb",
        "jwt",
        "postman",
        "i18n",
        "jwt-authentication",
        "token",
        "eslint",
        "starter",
        "web",
        "app",
        "mongoose",
        "rest",
        "skeleton",
        "async",
        "await",
        "mvp",
        "front-end",
        "testing",
        "prettier",
        "mocha",
        "chai",
        "redis",
        "JSDoc"
    ]
}
